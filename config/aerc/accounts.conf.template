# /* vim: set ft=dosini: */
[Gmail]
source = imaps://<USER_NAME>@imap.gmail.com:993
source-cred-cmd = pass google.com/<app_passwd>
outgoing = smtps://<USER_NAME>@smtp.gmail.com:587
outgoing-cred-cmd = pass google.com/<app_passwd>
default = INBOX
from = REAL_NAME <<USER_NAME>@gmail.com>
cache-headers = true
